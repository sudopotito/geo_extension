{
  "branches": [
    "main"
  ],
  "tagFormat": "v${version}",
  "repositoryUrl": "https://github.com/sudopotito/geo_extension",
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular"
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "angular"
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "sed -ir \"s/[0-9]*\\.[0-9]*\\.[0-9]*/${nextRelease.version}/\" geo_extension/__init__.py"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "geo_extension/__init__.py"
        ],
        "message": "chore(release): Bumped to Version ${nextRelease.version}"
      }
    ],
    "@semantic-release/github"
  ]
}
